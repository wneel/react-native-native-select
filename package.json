	"license": "MIT",
	"bugs": {
		"url": "https://github.com/wneel/react-native-native-select/issues"
	},
	"homepage": "https://github.com/wneel/react-native-native-select#readme",
	"peerDependencies": {
		"react": "*",
		"react-native": ">=0.71.0"
	},
	"codegenConfig": {
		"name": "RTNSelectSpec",
